{
  "name": "pretty-logs",
  "version": "0.1.0",
  "description": "Welcome to pretty-logs.  A logging tool to help developers create more informative logs.",
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/tkjone/pretty-logs"
  },
  "keywords": [
    "logging",
    "logs",
    "pretty"
  ],
  "bugs": {
    "url": "https://github.com/tkjone/pretty-logs/issues"
  },
  "homepage": "https://github.com/tkjone/pretty-logs#readme",
  "author": "tkjone <tm.mattacchione@gmail.com> (http://thomasmattacchione.com)",
  "license": "MIT",
  "engines": {
    "node": "6.x",
    "npm": "3.x"
  },
  "scripts": {
    "build": "",
    "lint": "eslint .",
    "precommit": "lint-staged",
    "format": "prettier-eslint --write \"src/**/*.js\"",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "babel-jest": "^19.0.0",
    "babel-preset-es2015": "^6.24.1",
    "husky": "^0.13.3",
    "jest-cli": "^19.0.2",
    "lint-staged": "^3.4.1",
    "prettier-eslint-cli": "^3.4.2",
    "regenerator-runtime": "^0.10.5"
  },
  "babel": {
      "presets": [
        "es2015"
      ]
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true
    },
    "rules": {}
  },
  "jest": {
    "coverageDirectory": "./tools/jest/coverage-reports",
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    }
  }
}
